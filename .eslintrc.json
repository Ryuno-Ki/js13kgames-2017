{
  "env": {
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "array-callback-return": 2,
    "block-scoped-var": 2,
    "class-methods-use-this": 2,
    "consistent-return": 2,
    "default-case": 2,
    "dot-notation": 2,
    "eqeqeq": 2,
    "getter-return": 2,
    "no-case-declarations": 2,
    "no-console": 1,
    "no-dupe-args": 2,
    "no-duplicate-case": 2,
    "no-dupe-keys": 2,
    "no-else-return": 2,
    "no-empty": 2,
    "no-empty-function": 2,
    "no-empty-pattern": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-label": 2,
    "no-extra-semi": 2,
    "no-eq-null": 2,
    "no-fallthrough": 2,
    "no-floating-decimal": 2,
    "no-func-assign": 2,
    "no-global-assign": 2,
    "no-implicit-coercion": 2,
    "no-implicit-globals": 2,
    "no-inner-declarations": 2,
    "no-invalid-this": 2,
    "no-irregular-whitespace": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-magic-numbers": 1,
    "no-multi-spaces": 1,
    "no-multi-str": 2,
    "no-new": 2,
    "no-new-func": 2,
    "no-new-wrappers": 2,
    "no-octal": 2,
    "no-param-reassign": 2,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-restricted-properties": 2,
    "no-return-assign": 2,
    "no-self-assign": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-shadow": 2,
    "no-shadow-restricted-names": 2,
    "no-template-curly-in-string": 2,
    "no-undef": 2,
    "no-undef-init": 2,
    "no-undefined": 2,
    "no-unexpected-multiline": 2,
    "no-unreachable": 2,
    "no-unsafe-negation": 2,
    "no-unused-expressions": 2,
    "no-unused-labels": 2,
    "no-unused-vars": 1,
    "no-use-before-define": 2,
    "no-useless-concat": 2,
    "no-useless-escape": 2,
    "no-useless-return": 2,
    "no-void": 2,
    "radix": 2,
    "semi": 2,
    "strict": 2,
    "valid-typeof": 2,
    "wrap-iife": 2,
    "yoda": 2
  }
}
